# Workers count (don't set too high without proxies)
MAX_GOROUTINES=10

# GitHub session credentials (from browser cookies)
MAIN_ACCOUNT_COOKIE="your_github_cookie_here"
MAIN_ACCOUNT_AUTH_TOKEN="your_auth_token_here"

# Features
ENABLE_USERNAME_CHANGE=false
ENABLE_TELEGRAM_MESSAGE_IF_404=false

# Retry settings
RETRY_COUNT=5

# Data directory path
FILES_PATH=data

# Telegram notifications (optional)
TG_BOT_TOKEN="your_telegram_bot_token"
TG_USER_ID=0
